<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="borderPane" minHeight="500.0" minWidth="750.0"
	xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.esf.xmlParser.fxml.MainController">
	<top>
		<VBox fx:id="borderPaneTop">
			<children>
				<MenuBar fx:id="menubar">
					<menus>
						<Menu mnemonicParsing="false" text="%menuItemFile">
							<items>
								<MenuItem fx:id="menuItemChooseFile" mnemonicParsing="false"
									onAction="#menuItemChooseFile" text="%menuItemChooseFile" />
								<SeparatorMenuItem mnemonicParsing="false" />
								<MenuItem fx:id="menuItemClose" mnemonicParsing="false"
									onAction="#menuItemClose" text="%menuItemClose" />
							</items>
						</Menu>
						<Menu mnemonicParsing="false" text="%menuItemHelp">
							<items>
								<MenuItem fx:id="menuItemAbout" mnemonicParsing="false"
									onAction="#menuItemAbout" text="%menuItemAbout" />
							</items>
						</Menu>
					</menus>
				</MenuBar>
				<AnchorPane>
					<children>
						<ComboBox fx:id="comboBoxSearch" editable="true"
							onAction="#comboBoxItemSelected" prefWidth="150.0"
							AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
							AnchorPane.rightAnchor="76.0" AnchorPane.topAnchor="0.0" />
						<Button fx:id="buttonSearch" defaultButton="true"
							maxHeight="-Infinity" minHeight="-Infinity" mnemonicParsing="false"
							onAction="#buttonSearchClick" prefHeight="26.0" text="%search"
							AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0"
							AnchorPane.topAnchor="0.0" />
					</children>
				</AnchorPane>
			</children>
		</VBox>
	</top>
</BorderPane>
